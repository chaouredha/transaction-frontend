  "private": true,
  "devDependencies": {
    "@babel/core": "7.13.1",
    "@babel/plugin-transform-runtime": "7.13.7",
    "@babel/preset-env": "7.13.5",
    "@babel/preset-react": "7.12.13",
    "@babel/runtime": "7.13.7",
    "@babel/runtime-corejs3": "7.13.7",
    "@jest/globals": "26.6.2",
    "@testing-library/react": "11.2.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.1.0",
    "eslint": "7.20.0",
    "eslint-config-standard-jsx": "10.0.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.2.0",
    "husky": "5.1.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-styled-components": "7.0.3",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.3.9",
    "npm-check-updates": "11.1.9",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "prettier": "2.2.1",
    "react-test-renderer": "17.0.1",
    "source-map-loader": "2.0.1",
    "styled-components": "5.2.1",
    "terser-webpack-plugin": "5.1.1",
    "webpack": "5.24.2",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaouredha/gestion-contact-frontend.git"
  },
  "author": "chaou.redha@gmail.com",
  "bugs": {
    "url": "https://github.com/chaouredha/gestion-contact-frontend/issues"
  },
  "dependencies": {
    "@tippyjs/react": "4.2.1",
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "core-js": "3.9.0",
    "date-fns": "2.17.0",
    "gh-pages": "^3.1.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-day-picker": "7.4.8",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-select": "4.1.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-all": "jest",
    "lint": "eslint \"**/*.js\"",
    "ncuu": "ncu -u",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "^(actions)/(.*)$": "<rootDir>/src/redux/$1/$2",
      "^(api)/(.*)$": "<rootDir>/src/$1/$2",
      "^(app)/(.*)$": "<rootDir>/src/$1/$2",
      "^(components)/(.*)$": "<rootDir>/src/$1/$2",
      "^(domain)/(.*)$": "<rootDir>/src/$1/$2",
      "^(images)/(.*)$": "<rootDir>/src/$1/$2",
      "^(reducers)/(.*)$": "<rootDir>/src/redux/$1/$2"
    }
  },
  "lint-staged": {
    "*.(js|html|css)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "^12"
  }
}
